
jQuery(window).load(function($){console.log('gallery loading function >> start');jQuery('#syg_video_gallery-1').removeClass('syg_video_gallery_loading-1').addClass('syg_video_gallery-1');jQuery('.sc_menu-1').removeAttr('style');jQuery(function($){var div=$('div.sc_menu-1'),ul=$('ul.sc_menu-1'),ulPadding=15;var divWidth=450;div.css({overflow:'hidden'});var lastLi=ul.find('li:last-child');if($.fn.sygclient('isMobileBrowser')){div.bind('touchmove',function(jQueryEvent){jQueryEvent.preventDefault();var event=window.event;var ulWidth=lastLi[0].offsetLeft+lastLi.outerWidth()+ulPadding;var left=(event.touches[0].pageX-div.offset().left)*(ulWidth-divWidth)/divWidth;div.scrollLeft(left);});}else{div.mousemove(function(e){var ulWidth=lastLi[0].offsetLeft+lastLi.outerWidth()+ulPadding;var left=(e.pageX-div.offset().left)*(ulWidth-divWidth)/divWidth;div.scrollLeft(left);});}});if(jQuery.fn.sygclient('isMobileBrowser',this)){jQuery.mobile.hidePageLoadingMsg();}
console.log('gallery loading function >> end');});